{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-ms-text-size-adjust"
        ]
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-viewport": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 106",
    "not and_ff <= 105",
    "not chrome <= 106",
    "not edge <= 106",
    "not firefox <= 105",
    "not ios_saf <= 16",
    "not opera <= 91",
    "not safari <= 16",
    "not samsung <= 18"
  ]
}